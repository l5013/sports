<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lal.sp.dao.IAuthDao">
	<!--
		这里namespace必须是UserMapper接口的路径，不然要运行的时候要报错 “is not known to the
		MapperRegistry”
	-->
	<select id="login" parameterType="map" resultType="Admin">
		select * from sp_admin where username=#{username} and password=#{password}
	</select>
</mapper>